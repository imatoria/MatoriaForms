{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CaptchaOptions": {
    "CaptchaType": 5,
    "CodeLength": 4,
    "ExpirySeconds": 120,
    "IgnoreCase": true,
    "StoreageKeyPrefix": "CaptchaCode",
    "ImageOption": {
      "Animation": false,
      "FontSize": 32,
      "Width": 100,
      "Height": 35,
      "BubbleMinRadius": 5,
      "BubbleMaxRadius": 10,
      "BubbleCount": 3,
      "BubbleThickness": 1.0,
      "InterferenceLineCount": 3,
      "FontFamily": "Verdana",
      "FrameDelay": 15,
      "BackgroundColor": "#ffffff",
      "ForegroundColors": "",
      "Quality": 100,
      "TextBold": false
    }
  }
}
